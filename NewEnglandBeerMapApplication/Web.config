  </configSections>
  <connectionStrings>    
    <add name="DefaultConnection" connectionString="server=[database server address];user id=[user id name];password=[password];database=[database name];" providerName="MySql.Data.MySqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="GoogleMapsAPIKey" value="%%Insert%Google%Maps%API%Key%%" />
    <add key="GoogleGeocodingAPIKey" value="%%Insert%Google%Geocoding%API%Key%%" />
    <add key="recaptchaPublicKey" value="%%Insert%recaptcha%Public%Key%%" />
    <add key="recaptchaPrivateKey" value="%%recaptcha%Private%Key%%" />
    <add key="recaptchaApiVersion" value="2" />
  </appSettings>
  <system.web>
    <compilation debug="false" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
    <customErrors mode="Off" />
    <trust level="Full" />
  </system.web>
  <system.net>
    <mailSettings>
      <smtp from="%%From%Email%Address%%">
        <network host="relay-hosting.secureserver.net" port="25" />
      </smtp>
    </mailSettings>
  </system.net>
